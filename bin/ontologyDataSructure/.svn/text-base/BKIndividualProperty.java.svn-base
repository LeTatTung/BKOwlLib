package ontologyDataSructure;

import java.util.ArrayList;

/**
 * Lưu trữ giá trị của 1 property của 1 instance
 * @author DatTT
 *
 */
public class BKIndividualProperty {
	
	private String property;
	private ArrayList<String> listValue;
	public String getProperty() {
		return property;
	}
	public void setProperty(String property) {
		this.property = property;
	}
	public ArrayList<String> getListValue() {
		return listValue;
	}
	public void setListValue(ArrayList<String> listValue) {
		this.listValue = listValue;
	}
	
	public BKIndividualProperty()
	{
		
	}
	
	public BKIndividualProperty(String property, ArrayList<String> listValue) {
		super();
		this.property = property;
		this.listValue = listValue;
	}
	
}
